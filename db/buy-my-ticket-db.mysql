INSERT INTO `locales_source` VALUES ('4272','','default','Optional subdirectory within the upload destination where files will be stored. Do not include preceding or trailing slashes.','','7.7'),('4273','','default','Enter a value like "512" (bytes), "80 KB" (kilobytes) or "50 MB" (megabytes) in order to restrict the allowed file size. If left empty the file sizes will be limited only by PHP''s maximum post and file upload sizes (current limit <strong>%limit</strong>).','','7.7'),('4274','','default','Enable <em>Description</em> field','','7.7'),('4275','','default','The description field allows users to enter a description about the uploaded file.','','7.7'),('4276','','default','The "!name" option must contain a valid value. You may either leave the text field empty or enter a string like "512" (bytes), "80 KB" (kilobytes) or "50 MB" (megabytes).','','none'),('4277','','default','The list of allowed extensions is not valid, be sure to exclude leading dots and to separate extensions with a comma or space.','','none'),('4278','','default','Add a new file','','none'),('4279','','default','Include file in display','','none'),('4280','','default','The description may be used as the label of the link to the file.','','none'),('4281','','default','Files must be less than !size.','','7.7'),('4282','','default','Allowed file types: !extensions.','','7.7'),('4283','','default','Images must be exactly !size pixels.','','none'),('4284','','default','Images must be between !min and !max pixels.','','none'),('4285','','default','Images must be larger than !min pixels.','','none'),('4286','','default','Images must be smaller than !max pixels.','','none'),('4287','','default','An unrecoverable error occurred. Use of this form has expired. Try reloading the page and submitting again.','','none'),('4288','','default','The file referenced by the !name field does not exist.','','none'),('4289','','default','The file in the !name field was unable to be uploaded.','','none'),('4290',''