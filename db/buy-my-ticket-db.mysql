INSERT INTO `locales_source` VALUES ('3706','','default','Consider increasing your PHP memory limit to %memory_minimum_limit to help prevent errors in the installation process.','','none'),('3707','','default','Consider increasing your PHP memory limit to %memory_minimum_limit to help prevent errors in the update process.','','none'),('3708','','default','Depending on your configuration, Drupal can run with a %memory_limit PHP memory limit. However, a %memory_minimum_limit PHP memory limit or above is recommended, especially if your site uses additional custom or contributed modules.','','none'),('3709','','default','Increase the memory limit by editing the memory_limit parameter in the file %configuration-file and then restart your web server (or contact your system administrator or hosting provider for assistance).','','none'),('3710','','default','Contact your system administrator or hosting provider for assistance with increasing your PHP memory limit.','','none'),('3711','','default','See the <a href="@url">Drupal requirements</a> for more information.','','none'),('3712','','default','Not protected','','none'),('3713','','default','The file %file is not protected from modifications and poses a security risk. You must change the file''s permissions to be non-writable.','','none'),('3714','','default','For more information, see the online handbook entry for <a href="@cron-handbook">configuring cron jobs</a>.','','7.7'),('3715','','default','Cron has not run recently.','','none'),('3716','','default','Cron maintenance tasks','','7.7'),('3717','','default','You can <a href="@cron">run cron manually</a>.','','7.7'),('3718','','default','The directory %directory is not writable.','','none'),('3719','','default','You may need to set the correct directory at the <a href="@admin-file-system">file system settings page</a> or change the current directory''s permissions so that it is writable.','','none'),('3720','','default','Writable (<em>public</em> download method)','