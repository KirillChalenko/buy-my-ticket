INSERT INTO `locales_source` VALUES ('4067','','default','The URL %url is invalid. Enter a fully-qualified URL, such as http://www.example.com/feed.xml.','','none'),('4068','','default','A feed named %feed already exists. Enter a unique title.','','none'),('4069','','default','A feed with this URL %url already exists. Enter a unique URL.','','none'),('4070','','default','Upload an OPML file containing a list of feeds to be imported.','','none'),('4071','','default','OPML Remote URL','','none'),('4072','','default','Enter the URL of an OPML file. This file will be downloaded and processed only once on submission of the form.','','none'),('4073','','default','Drupal can make a block with the most recent news items of a feed. You can <a href="@block-admin">configure blocks</a> to be displayed in the sidebar of your page. This setting lets you configure the number of news items to show in a feed''s block. If you choose ''0'' these feeds'' blocks will be disabled.','','none'),('4074','','default','You must <em>either</em> upload a file or enter a URL.','','none'),('4075','','default','This URL is not valid.','','none'),('4076','','default','No new feed has been added.','','none'),('4077','','default','The URL %url is invalid.','','none'),('4078','','default','A feed named %title already exists.','','none'),('4079','','default','A feed with the URL %url already exists.','','none'),('4080','','default','Fetchers download data from an external source. Choose a fetcher suitable for the external source you would like to download from.','','none'),('4081','','default','Parsers transform downloaded data into standard structures. Choose a parser suitable for the type of feeds you would like to aggregate.','','none'),('4082','','default','Processors act on parsed feed data, for example they store feed items. Choose the processors suitable for your task.','','none'),('4083','','default','For most aggregation tasks, the default settings are fine.','','none'),('4084','','default','A