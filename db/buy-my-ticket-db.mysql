INSERT INTO `locales_source` VALUES ('4404','','default','Height and width values must be numeric.','','none'),('4405','','default','Both a height and width value must be specified in the !name field.','','none'),('4406','','default','Preview image style','','7.7'),('4407','','default','no preview','','7.7'),('4408','','default','The preview image will be shown while editing the content.','','7.7'),('4409','','default','Image styles commonly provide thumbnail sizes by scaling and cropping images, but can also add various effects before an image is displayed. When an image is displayed with a style, a new file is created and the original image is left unchanged.','','7.7'),('4410','','default','No defined styles','','none'),('4411','','default','Image generation in progress. Try again shortly.','','none'),('4412','','default','Error generating image.','','none'),('4413','','default','Unable to generate the derived image located at %path.','','none'),('4414','','default','Failed to create style directory: %directory','','none'),('4415','','default','Cached image file %destination already exists. There may be an issue with your rewrite configuration.','','none'),('4416','','default','Generate image style','','7.7'),('4417','','default','Configure an image style.','','7.7'),('4418','','default','Delete an image style.','','none'),('4419','','default','Revert style','','none'),('4420','','default','Revert an image style.','','none'),('4421','','default','Edit image effect','','7.7'),('4422','','default','Delete image effect','','none'),('4423','','default','Add image effect','','none'),('4424','','default','Add a new effect to a style.','','none'),('4425','','default','Use the <em>Custom language</em> section below if your desired language does not appear in this list.','','none'),('4426','','default','Path prefix language code','','none'),('4427','','default','Language code or other custom text to use as a path prefix for URL language detection, if your <em>Detection and selection</em> settings use URL path prefixes. For the default language, this value may be left blank. <strong>Modifying this value may break existing URLs. Use with caution in a production environment.</strong> Example: Specifying "deutsch" as the path prefix code for German results in URLs like "example.com/deutsch/contact".','','none');
INSERT INTO `locales_source` VALUES ('4428','','default','Detection method','','none'),('4429','','default','Part of the URL that determines language','','none'),('4430','','default','<em>Path prefix</em>: URLs like http://example.com/de/contact set language to German (de). <em>Domain</em>: URLs like http://de.example.com/contact set the language to German. <strong>Warning: Changing this setting may break incoming URLs. Use with caution on a production site.</strong>','','none'),('4431','','default','Request/session parameter','','none'),('4432','','default','Name of the request/session parameter used to determine the desired language.','','none'),('4433','','default','Date type','','7.7'),('4434','','default','Are you sure you want to reset the date formats for %language to the global defaults?','','none'),('4435','','default','Resetting will remove all localized date formats for this language. This action cannot be undone.','','none'),('4436','','default','With multiple languages enabled, interface text can be translated, registered users may select their preferred language, and authors can assign a specific language to content. <a href="@translations">Download contributed translations</a> from Drupal.org.','','none'),('4437','','default','Add a language to be supported by your site. If your desired language is not available in the <em>Language name</em> drop-down, click <em>Custom language</em> and provide a language code and other details manually. When providing a language code manually, be sure to enter a standardized language code, since this code may be used by browsers to determine an appropriate display language.','','none'),('4438','','default','Determine the language from a request/session parameter. Example: "http://example.com?language=de" sets language to German based on the use of "de" within the "language" parameter.','','none'),('4439','','default','Localize date formats','','7.7'),('4440','','default','Reset date formats','','none'),('4441','','de