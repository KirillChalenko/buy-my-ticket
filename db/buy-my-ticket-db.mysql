INSERT INTO `locales_target` VALUES ('1549','Заметьте, что файлы <em>.po</em> из пакета перевода импортируются автоматически (по возможности), когда подключаются новые модули или темы оформления. Так как эта страница позволяет одновременно закачивать только один <em>.po</em> файл, возможно, будет проще скачать и распаковать пакет перевода в ваш корневой каталог Drupal и <a href="@language-add">добавить язык</a> (это автоматически импортирует все <em>.po</em> файлы пакета). Пакеты перевода доступны для скачивания на <a href="@translations">странице переводов Drupal</a>.','ru','0','0'),('1563','Управление блоками','ru','0','0'),('1564','Управление комментриями и настройка комментариев','ru','0','0'),('1565','Просмотр комментариев','ru','0','0'),('1566','Добавление комментариев','ru','0','0'),('1567','Добавление комментариев без проверки','ru','0','0'),('1568','Правка своих комментариев','ru','0','0'),('1569','Управление формами контактов и настройки формы контактов','ru','0','0'),('1570','Доступ к контактной форме сайта','ru','0','0'),('1571','Использовать пользовательские личные контактные формы','ru','0','0'),('1572','Использование контекстных ссылок','ru','0','0'),('1573','Используйте контекстные ссылки для выполнения действий, связанных с элементами страницы.','ru','0','0'),('1574','Открыть административную панель','ru','0','0');
